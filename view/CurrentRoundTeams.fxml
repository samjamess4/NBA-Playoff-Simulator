<VBox fx:controller = "controller.CurrentRoundTeamsController" styleClass="background"  alignment="CENTER" prefHeight="300" prefWidth="400" spacing="20" xmlns:fx="http://javafx.com/fxml/1" >
   

    <HBox alignment="CENTER">
        <Label text="Round: 1" styleClass="round-text"/>
    </HBox>


    <TableView fx:id ="gamesTable">
        <columns>
            <TableColumn fx:id= "name1"    text="Team 1" />
            <TableColumn text="VS" />
            <TableColumn text="Team 2" fx:id="name2" />
        </columns>
    </TableView>

    <Button onAction="#close" text="Close" />


    <stylesheets>
        <URL value="@style.css"/>
    </stylesheets>
</VBox>